{
  "name": "questrade-summary-server",
  "version": "1.0.0",
  "private": true,
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon --ignore token-store.json --ignore data/qqq-cache/** --ignore earliest-funding-cache.json --ignore '.cache/**' src/index.js",
    "seed-token": "node scripts/seed-refresh.js",
    "test": "node --test",
    "test:pnl-by-symbol": "node --test ./test/totalPnlBySymbol.test.js",
    "test:unit": "node --test ./test/accountNamesTopLevelRelations.test.js ./test/accountNamesGroupRelations.test.js ./test/assignAccountGroupsHierarchy.test.js",
